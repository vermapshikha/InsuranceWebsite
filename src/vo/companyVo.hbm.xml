<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="vo.companyVo" table="Company">
      <id name="company_ID" type="int" column="company_ID">
         <generator class="increment"/>
      </id>
      <property name="companyName"> 
      <column name="companyName"/></property>
     <property name="companyDes"> 
      <column name="companyDescription"/></property>
      <property name="companyStatus">
      <column name="companyStatus"/></property>
     
     
   </class>
</hibernate-mapping>